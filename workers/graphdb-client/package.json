{
  "name": "@graphdb/client",
  "version": "1.0.0",
  "description": "GraphDB client for Cloudflare Workers with GraphQL/REST support, automatic retries, and timeout handling",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json && for f in dist/esm/*.js; do mv \"$f\" \"${f%.js}.mjs\"; done && cp dist/esm/*.mjs dist/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "graphdb",
    "cloudflare-workers",
    "graphql",
    "rest-api",
    "client",
    "graph-database",
    "syntopica",
    "cluso",
    "trust-score",
    "fraud-detection"
  ],
  "author": "Cluso Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dd0wney/graphdb.git",
    "directory": "workers/graphdb-client"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231025.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
